{
  "kind": "collectionType",
  "collectionName": "tags",
  "info": {
    "singularName": "tag",
    "pluralName": "tags",
    "displayName": "Tag",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "minLength": 2,
      "maxLength": 60
    },
    "code": {
      "type": "uid",
      "targetField": "name",
      "required": true
    },
    "tagEmoji": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::tag-emoji.tag-emoji",
      "inversedBy": "tags"
    },
    "group": {
      "type": "enumeration",
      "enum": ["taste", "diet", "audience", "other"],
      "required": true
    },
    "priority": {
      "type": "integer",
      "default": 0
    },
    "is_active": {
      "type": "boolean",
      "default": true
    },
    "is_filterable": {
      "type": "boolean",
      "default": true
    },
    "description": {
      "type": "text"
    },
    "kinzas": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::kinza.kinza",
      "mappedBy": "tags"
    }
  }
}
