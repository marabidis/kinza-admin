{
  "kind": "collectionType",
  "collectionName": "categories",
  "info": {
    "singularName": "category",
    "pluralName": "categories",
    "displayName": "Категории"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "order": {
      "type": "integer"
    },
    "emoji": {
      "type": "string"
    },
    "catalogLayout": {
      "type": "enumeration",
      "enum": [
        "list",
        "grid_2"
      ],
      "required": true,
      "default": "list"
    },
    "detailLayout": {
      "type": "enumeration",
      "enum": [
        "standard",
        "full_screen"
      ],
      "required": true,
      "default": "standard"
    },
    "cutleryEligibleDefault": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "cutlerySetsDefault": {
      "type": "integer",
      "required": true,
      "min": 0,
      "default": 1
    },
    "products": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::kinza.kinza",
      "inversedBy": "categories"
    }
  }
}
