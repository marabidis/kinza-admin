{
  "extends": "@strapi/typescript-utils/tsconfigs/server",
  "compilerOptions": {
    "outDir": "dist",
    "rootDir": ".",
    "allowJs": true
  },
  "include": [
    "./**/*.ts",
    "./**/*.js",
    "src/**/*.json",
    "plugins/**/*"
  ],
  "exclude": [
    "node_modules",
    "build",
    "dist",
    ".cache",
    ".tmp",
    "src/admin",
    "**/*.test.*"
  ]
}